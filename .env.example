# Auth Service Configuration

# Auth Database Password
# Generate with: openssl rand -base64 32
AUTH_DB_PASSWORD=change_this_password

# Google OAuth 2.0
# Get from: https://console.cloud.google.com/
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# JWT Secret
# Generate with: openssl rand -base64 64
JWT_SECRET=your_jwt_secret

# Session Secret
# Generate with: openssl rand -base64 32
SESSION_SECRET=your_session_secret

# Initial Admin Email
INITIAL_ADMIN_EMAIL=your_email@gmail.com

# Real Estate Calculator Database (existing)
SPRING_PROFILES_ACTIVE=dev
REALESTATE_DB_URL=jdbc:postgresql://host.docker.internal:5432/realestate
REALESTATE_DB_USERNAME=postgres
REALESTATE_DB_PASSWORD=postgres
